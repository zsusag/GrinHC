data Ctor =
 | First Float
 | Second Int Float
 | Third Int Float Int
 | Fourth Int Float Int Int

let z :: Ctor = Third 5 2.3 3
in case z of
  (First x) -> ( x )
  (Second x1 x2) -> ( x1 + x2 )
  (Third x1 x2 x3) -> ( x1 + x2 + x3 )
  (Fourth x1 x2 x3 x4) -> ( x1 + x2 + x3 + x4 )
  _ -> ( 3.2 )
end
